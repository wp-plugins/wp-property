      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    },
    {
      "name": "Maxim Peshkov",
      "email": "maxim.peshkov@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    },
    {
      "name": "Anton Korotkov",
      "email": "anton.korotkov@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    }
  ],
  "extra": {
    "component": {
      "scripts":    [ "static/scripts/settings.js" ],
      "templates":  [ "static/views/new-installation.xhtml" ],
